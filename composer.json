{
    "name": "fabiang/mink-javascript-errors",
    "description": "Mink extension for handling JavaScript errors",
    "type": "behat-extension",
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.9",
        "phpunit/php-invoker": "^1.1",
        "behat/behat": "^3.0.15",
        "behat/mink": "^1.7",
        "behat/mink-extension": "^2.1",
        "behat/mink-selenium2-driver": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Fabiang\\Mink\\JavaScriptErrors\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fabiang\\Mink\\JavaScriptErrors\\Behat\\": "tests/features/bootstrap/"
        }
    },
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Fabian Grutschus",
            "email": "f.grutschus@lubyte.de"
        }
    ],
    "minimum-stability": "stable"
}
